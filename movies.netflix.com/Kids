})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_5").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_5","#slider_5 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_5 .previous", "#slider_5 .next", "", "#slider_5 .agMovieSetSlider .boxShotDivider"
                    ,"#slider_5 .next","#slider_5 .previous" ).init($parent);

            
            
            sm.rowNumber = Number(4);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_5","sliderReady",setupSliders);


