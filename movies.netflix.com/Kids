      cssId: '#b070037410_0',
      seen: false
    });

    netflix.uitracking.Title.update('b070037410_0', {
      video_id: '70037410',
      track_id: '50000108',
      row: '7',
      rank: '39',
      request_id: netflix.constants.page.REQUEST_ID,
      location: netflix.constants.page.SiteSection,
      time: 0
    });
  }
  catch (e) {
    window.console && console.error && console.error('Failed to update ', e);
  }
})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_8").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_8","#slider_8 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_8 .previous", "#slider_8 .next", "", "#slider_8 .agMovieSetSlider .boxShotDivider"
                    ,"#slider_8 .next","#slider_8 .previous" ).init($parent);

            
            
            sm.rowNumber = Number(7);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_8","sliderReady",setupSliders);


