        var setupSliders = function() {

            var $parent = $("#slider_11").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_11","#slider_11 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_11 .previous", "#slider_11 .next", "", "#slider_11 .agMovieSetSlider .boxShotDivider"
                    ,"#slider_11 .next","#slider_11 .previous" ).init($parent);

            
            
            sm.rowNumber = Number(10);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_11","sliderReady",setupSliders);


