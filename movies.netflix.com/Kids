        var setupSliders = function() {

            var $parent = $("#slider_7").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_7","#slider_7 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_7 .previous", "#slider_7 .next", "", "#slider_7 .agMovieSetSlider .boxShotDivider"
                    ,"#slider_7 .next","#slider_7 .previous" ).init($parent);

            
            
            sm.rowNumber = Number(6);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_7","sliderReady",setupSliders);


