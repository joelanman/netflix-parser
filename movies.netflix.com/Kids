})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_4").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_4","#slider_4 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_4 .previous", "#slider_4 .next", "", "#slider_4 .agMovieSetSlider .boxShotDivider"
                    ,"#slider_4 .next","#slider_4 .previous" ).init($parent);

            
            
            sm.rowNumber = Number(3);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_4","sliderReady",setupSliders);


