        var setupSliders = function() {

            var $parent = $("#slider_9").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_9","#slider_9 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_9 .previous", "#slider_9 .next", "", "#slider_9 .agMovieSetSlider .boxShotDivider"
                    ,"#slider_9 .next","#slider_9 .previous" ).init($parent);

            
            
            sm.rowNumber = Number(8);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_9","sliderReady",setupSliders);


