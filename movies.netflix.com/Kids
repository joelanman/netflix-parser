    });
  }
  catch (e) {
    window.console && console.error && console.error('Failed to update ', e);
  }
})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_3").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_3","#slider_3 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_3 .previous", "#slider_3 .next", "", "#slider_3 .agMovieSetSlider .boxShotDivider"
                    ,"#slider_3 .next","#slider_3 .previous" ).init($parent);

            
            
            sm.rowNumber = Number(2);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_3","sliderReady",setupSliders);


