	})(jQuery);
				    (function($){
            var _$picker, _$rows, _$links, _selected,
            _select = function(index) {
                // Highlight the selected link in the friend picker and switch to displaying the list of videos associated with that link.

                var stretchyManager;
                if (_selected) {
                    _selected.$row.hide();
                    _selected.$link.removeClass("selected");
                }
                _selected = {
                    $row: _$rows.eq(index),
                    $link: _$links.eq(index)
                };
                _selected.$row.show();
                _selected.$link.addClass("selected");

                // Notify the StretchyManager that its display state has changed, so it should show
                // boxshots and recalculate whether it is scrollable
                stretchyManager = _selected.$row.data("sliderManager");
                if (stretchyManager) {
                    stretchyManager.refresh();
                }
            },
            _initialize = function() {
                _$picker = $("#fbFriendPicker_0");
                _$rows = $("div.collated.mrow", _$picker.closest("div.mrow"));
                _$links = $("li", _$picker);

                // Each link in the friend picker is associated with the collated row with the same index
                // (e.g., hovering over the 3rd friend in the list should display the 3rd collated row).
                $("li a", _$picker).each(function(index) {
                    var select = function(evt) {
                        evt.preventDefault();
                        _select(index);
                        return false;
                    };
                    $(this).mouseover(select).click(select);
                });

                // On page load, start with the first link highlighted and the first list of videos displayed.
                _select(0);
            };
            _initialize();
        })(jQuery);
