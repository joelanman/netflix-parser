        var setupSliders = function() {

            var $parent = $("#slider_2").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_2","#slider_2 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_2 .previous", "#slider_2 .next", "", "#slider_2 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(2);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_2","sliderReady",setupSliders);


