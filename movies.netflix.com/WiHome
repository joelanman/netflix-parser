})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_18").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_18","#slider_18 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_18 .previous", "#slider_18 .next", "", "#slider_18 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(17);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_18","sliderReady",setupSliders);


