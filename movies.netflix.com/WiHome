        var setupSliders = function() {

            var $parent = $("#slider_13").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_13","#slider_13 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_13 .previous", "#slider_13 .next", "", "#slider_13 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(12);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_13","sliderReady",setupSliders);


