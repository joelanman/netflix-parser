        var setupSliders = function() {

            var $parent = $("#slider_17").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_17","#slider_17 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_17 .previous", "#slider_17 .next", "", "#slider_17 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(16);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_17","sliderReady",setupSliders);


