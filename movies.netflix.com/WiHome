})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_19").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_19","#slider_19 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_19 .previous", "#slider_19 .next", "", "#slider_19 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(18);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_19","sliderReady",setupSliders);


