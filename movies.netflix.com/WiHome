})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_6").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_6","#slider_6 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_6 .previous", "#slider_6 .next", "", "#slider_6 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(4);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_6","sliderReady",setupSliders);


})(jQuery);(function($){
		var $row = $("#slider_6").closest("div.mrow").get(0);
		if ($row) {
			$.data($row,"_dataProvider", new netflix.sliders.StretchyDataProvider(
				"#slider_6 .agMovieSetSlider",
