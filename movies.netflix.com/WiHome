    netflix.uitracking.Title.update('b070137777_1', {
      video_id: '70137777',
      track_id: '50000817',
      row: '19',
      rank: '39',
      request_id: netflix.constants.page.REQUEST_ID,
      location: netflix.constants.page.SiteSection,
      time: 0
    });
  }
  catch (e) {
    window.console && console.error && console.error('Failed to update ', e);
  }
})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_20").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_20","#slider_20 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_20 .previous", "#slider_20 .next", "", "#slider_20 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(19);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_20","sliderReady",setupSliders);


