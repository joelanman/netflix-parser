        var setupSliders = function() {

            var $parent = $("#slider_15").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_15","#slider_15 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_15 .previous", "#slider_15 .next", "", "#slider_15 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(14);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_15","sliderReady",setupSliders);


