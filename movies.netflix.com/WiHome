})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_12").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_12","#slider_12 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_12 .previous", "#slider_12 .next", "", "#slider_12 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(11);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_12","sliderReady",setupSliders);


})(jQuery);(function($){
		var $row = $("#slider_12").closest("div.mrow").get(0);
		if ($row) {
			$.data($row,"_dataProvider", new netflix.sliders.StretchyDataProvider(
				"#slider_12 .agMovieSetSlider",
