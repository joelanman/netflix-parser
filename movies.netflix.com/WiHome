        var setupSliders = function() {

            var $parent = $("#slider_23").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_23","#slider_23 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_23 .previous", "#slider_23 .next", "", "#slider_23 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(22);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_23","sliderReady",setupSliders);


