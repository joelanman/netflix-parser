        var setupSliders = function() {

            var $parent = $("#slider_25").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_25","#slider_25 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_25 .previous", "#slider_25 .next", "", "#slider_25 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(24);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_25","sliderReady",setupSliders);


