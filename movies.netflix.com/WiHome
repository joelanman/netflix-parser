                    var videoId = MovieIdGetter($target), visitorIds = this.vmap[videoId], visitors = [], visitor;

                    
                    for (var i = 0; i < visitorIds.length && i < 6; i++) {
                        visitor = this.fbmap[visitorIds[i]];
                        if (visitor) {
                            visitors.push(visitor);
                        }
                    }

                    
                    if (!visitors.length) {
                        return {};
                    }

                    return {
                        
                        view: {
                            evidence: {
                                facepile: {
                                    visitors: visitors,
                                    likedBy: {
                                        firstVisitorName: visitors[0].name,
                                        totalVisitors: visitorIds.length
                                    }
                                }
                            }
                        },
                        
                        partials: {
                            likedBy: (visitors.length > 1 ? i18nStrings('video.evidence.social.friends.favorites.bob.likedBy.many.mustache') :
                                                            i18nStrings('video.evidence.social.friends.favorites.bob.likedBy.one.mustache'))
                        }
                    };
                }
            });

            
