    netflix.uitracking.Title.update('b016914901_0', {
      video_id: '16914901',
      track_id: '50007485',
      row: '8',
      rank: '39',
      request_id: netflix.constants.page.REQUEST_ID,
      location: netflix.constants.page.SiteSection,
      time: 0
    });
  }
  catch (e) {
    window.console && console.error && console.error('Failed to update ', e);
  }
})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_10").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_10","#slider_10 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_10 .previous", "#slider_10 .next", "", "#slider_10 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(8);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_10","sliderReady",setupSliders);


