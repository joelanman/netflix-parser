})();(function($){

        var setupSliders = function() {

            var $parent = $("#slider_1").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_1","#slider_1 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_1 .previous", "#slider_1 .next", "", "#slider_1 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(1);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_1","sliderReady",setupSliders);


