        var setupSliders = function() {

            var $parent = $("#slider_24").closest("div.mrow"),
                slider = new netflix.sliders.Infinite("#slider_24","#slider_24 .agMovieSetSlider", 4.0, "linear"),
			          sm = new netflix.sliders.StretchyManager( slider, "#slider_24 .previous", "#slider_24 .next", "", "#slider_24 .agMovieSetSlider .boxShotDivider"
                     ).init($parent);

            
            
            sm.rowNumber = Number(23);
            $parent.data("sliderManager", sm);
        };


        $("#bd").delegate("#slider_24","sliderReady",setupSliders);


